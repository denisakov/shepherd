<% title "Password reminder" %>
<h2><%= yield(:title)%></h2>

<%= form_for(resource, as: resource_name, url: password_path(resource_name), html: { method: :get }) do |f| %>
  <!--<%= devise_error_messages! %>-->


  <p><%= f.label :email %><br />
  <%= f.email_field :email, autofocus: true %></p>

  <p><%= f.submit "Send me reset password instructions", :class => "btn btn-lg btn-primary"  %></p>
<% end %>
<br />
<%= render "devise/shared/links" %>
